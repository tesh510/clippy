<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th>Title</th>
      <th>R<br>Rating</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% clips.forEach(function(m) { %>
      <tr>
        <td><%= m.R %></td>
        <td><%= m.Rating %></td>
        <td><a href="/clips/<%= m._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
